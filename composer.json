{
    "name": "dragonrun1/file_path_normalizer",
    "description": "File_path_normalizer is a class used to normalize PHP file paths without several of the shortcomings of the built-in functions.",
    "authors": [
        {
            "name": "Michael Cummings",
            "email": "mgcummings@yahoo.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "FilePathNormalizer\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Spec\\": "specs/"
        },
        "psr-4": {
            "FilePathNormalizer\\Test\\": [
                "tests/"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.*-dev"
        }
    },
    "license": "GPL-2.0",
    "minimum-stability": "stable",
    "require": {
        "php": ">5.4.1,<7.2"
    },
    "require-dev": {
        "dragonrun1/phpspec": "2.1.2",
        "henrikbjorn/phpspec-code-coverage": "1.0.1",
        "phpspec/nyan-formatters": "1.0.1",
        "phpunit/phpunit": "<5.2",
        "satooshi/php-coveralls": "dev-master"
    },
    "replace": {
        "dragonrun1/file_path_normalizer": "1.0.*"
    }
}
